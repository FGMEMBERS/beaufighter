  <crank>
   <name>crank</name>
   <mode>looped</mode>
   <path>Aircraft/beaufighter/Sounds/beaufighter_starter.wav</path>
   <condition>
    <property>sim/multiplay/generic/int[0]</property>
    <not>
     <property>/engines/engine/running</property>
    </not>
   </condition>
   <volume>
    <offset>-0.5</offset>
   </volume>
   <pitch>
    <property>engines/engine/rpm</property>
    <factor>0.0033</factor>
   </pitch>
   <position>
     <x>0.5</x>
     <y>0.00001</y>
     <z>-0.012</z>
   </position>
  </crank>

  <crank>
   <name>cough</name>
   <path>Aircraft/beaufighter/Sounds/beaufighter_startup.wav</path>
   <property>/engines/engine/running</property>
   <volume>
    <offset>0.15</offset>
   </volume>
   <pitch>
    <property>/engines/engine/running</property>
    <factor>0.15</factor>
   </pitch>
   <position>
     <x>0.5</x>
     <y>0.000001</y>
     <z>-0.012</z>
   </position>
  </crank>
  
  <engine>
   <name>engine</name>
   <mode>looped</mode>
   <path>Aircraft/beaufighter/Sounds/beaufighter_rpm1.wav</path>
   <condition>
    <property>/engines/engine/running</property>
    <less-than>
      <property>/engines/engine/rpm</property>
      <value>5000</value>
    </less-than>
    <greater-than>
      <property>/engines/engine/rpm</property>
      <value>700</value>
    </greater-than>
   </condition>
   <volume>
      <property>fdm/jsbsim/propulsion/engine/enclosure-attenuation</property>
      <!-- <min>0.5</min>
      <max>1.0</max> -->
      <offset>0.0</offset>
      <factor>1.0</factor>
   </volume>
   <volume>
    <property>/engines/engine/mp-inhg</property>
    <offset>0.0</offset>
    <factor>0.106666</factor>  
    <min>1.5</min>
    <max>8.0</max>
   </volume>
   <volume>
    <property>/engines/engine/rpm</property>
    <factor>0.00266666</factor>
    <min>1.3</min>
    <max>8.1</max>
    <offset>0.0</offset>
   </volume>
   <pitch>
    <property>/engines/engine/rpm</property>
    <factor>0.00025</factor>
    <min>0.0</min>
    <max>1.0</max>
   </pitch>
   <position>
     <x>0.5</x>
     <y>0.000001</y>
     <z>-0.012</z>
   </position>
  </engine>

  <engine>
   <name>enginerun</name>
   <mode>looped</mode>
   <path>Aircraft/beaufighter/Sounds/wasp.wav</path>
   <condition>
    <property>/engines/engine/running</property>
    <greater-than>
    <property>/engines/engine/rpm</property>
    <value>700</value>
    </greater-than>
   </condition>
   <volume>
      <property>fdm/jsbsim/propulsion/engine/enclosure-attenuation</property>
      <!-- <min>0.5</min>
      <max>1.0</max> -->
      <offset>0.0</offset>
      <factor>1.0</factor>
   </volume>
   <volume>
    <property>/engines/engine/mp-inhg</property>
    <offset>0.0</offset>
    <factor>0.02</factor>  
    <min>0.1</min>
    <max>5.6</max>
   </volume>
   <volume>
    <property>/engines/engine/rpm</property>
    <factor>0.0003</factor>
    <min>0.1</min>
    <max>5.6</max>
    <offset>0.0</offset>
   </volume>
   <pitch>
    <property>/engines/engine/rpm</property>
    <factor>0.00001</factor>
    <min>0.0</min>
    <max>0.70</max>
   </pitch>
   <position>
     <x>0.5</x>
     <y>0.000001</y>
     <z>-0.012</z>
   </position>
  </engine>

 <rumble>
   <name>rumble</name>
   <mode>looped</mode>
   <path>Aircraft/beaufighter/Sounds/rumble.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <type>log</type>
    <factor>0.25</factor>
   </volume>
   <position>
     <x>2.0</x>
     <y>0.000001</y>
     <z>-1.0</z>
   </position>
  </rumble>

  <gear>
   <name>gear up</name>
   <path>Aircraft/beaufighter/Sounds/gear-up.wav</path>
   <condition>
   <not>
       <property>/controls/gear/gear-down</property> 
   </not> 
   </condition>
   <volume>
    <property>/fdm/jsbsim/simulation/flapsSoundOK</property>
    <factor>1.5</factor>
   </volume>
   <position>
     <x>1.5</x>
     <y>0.000001</y>
     <z>-1.0</z>
   </position>
  </gear>
  
  <gear>
   <name>gear down</name>
   <path>Aircraft/beaufighter/Sounds/gear-down.wav</path>
   <property>/controls/gear/gear-down</property> 
   <volume>
    <property>/fdm/jsbsim/simulation/flapsSoundOK</property>
    <factor>1.5</factor>
   </volume>
   <position>
     <x>1.5</x>
     <y>0.000001</y>
     <z>-1.0</z>
   </position>
  </gear>

