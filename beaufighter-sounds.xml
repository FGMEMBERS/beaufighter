  <guns>
    <name>gunfire</name>
    <mode>looped</mode>
    <path>Aircraft/beaufighter/Sounds/gunfire_1.wav</path>
    <condition>
       <property>controls/armament/gun-trigger</property> 
       <!--<property>ai/submodels/trigger</property> -->
       <property>ai/submodels/submodel[1]/count</property>
       <property>/controls/armament/gunsight/power-on</property>
    </condition>
    <volume>
       <factor>1.2</factor>
       <max>1.0</max>
    </volume>
  </guns>
  
  <rockets>
    <name>rocket sound</name>
    <mode>once</mode>
    <path>Aircraft/beaufighter/Sounds/rockets.wav</path>
    <property>/fdm/jsbsim/systems/armament/rocket-sound</property>
    <volume>
       <factor>1.2</factor>
       <max>1.0</max>
    </volume>
  </rockets>

