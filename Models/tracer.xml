<?xml version="1.0"?>

<PropertyList>
  <path>tracer.ac</path>

  <animation>
    <type>dist-scale</type>
    <name>7.62 tracer</name>
    <object-name>tracer</object-name>
    <interpolation>
      <entry>
          <ind>0</ind>
          <dep>0.2</dep>
        </entry>
        <entry>
          <ind>400</ind>
          <dep>10</dep>
        </entry>
        <entry>
          <ind>1000</ind>
          <dep>0</dep>
        </entry>
    </interpolation>
 </animation>
  <animation>
    <type>dist-scale</type>
    <name>smoke trail</name>
    <object-name>trail</object-name>
    <interpolation>
      <entry>
          <ind>0</ind>
          <dep>0.8</dep>
        </entry>
        <entry>
          <ind>1600</ind>
          <dep>10</dep>
        </entry>
        <entry>
          <ind>4000</ind>
          <dep>0</dep>
        </entry>
    </interpolation>
 </animation>

 <animation>
   <type>material</type>
     <object-name>tracer</object-name>
     <emission>
       <red>0.8</red>
       <green>0.4</green>
       <blue>0.4</blue>
     </emission>
  </animation>

</PropertyList>
