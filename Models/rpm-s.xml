<?xml version="1.0"?>
  <!-- engine rpm instrument -->
  <PropertyList> 

    <path>rpm.ac</path>

    <animation>
      <type>rotate</type>
      <object-name>rpm-Needle1</object-name>
      <property>engines/engine[1]/rpm</property>
      <interpolation>
        <entry>
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry>
          <ind>5000</ind>
          <dep>180</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
      <animation> 
      <type>rotate</type>
      <object-name>rpm-Needle2</object-name>
      <property>engines/engine[1]/rpm</property>
      <interpolation>
        <entry>
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry>
          <ind>5000</ind>
          <dep>1800</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>

    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>

    <!-- *** LoD *** -->
    <animation>
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
