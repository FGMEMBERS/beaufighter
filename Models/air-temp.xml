<?xml version="1.0"?>
  <!-- Oil temp indicator -->
  <PropertyList> 
    <path>air-temp.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>needle</object-name>
      <property>environment/temperature-degc</property>
      <interpolation> 
        <entry> 
          <ind>-70</ind>
          <dep>-42.5</dep>
        </entry>
        <entry> 
          <ind>-60</ind>
          <dep>-33</dep>
        </entry>
        <entry> 
          <ind>-40</ind>
          <dep>-17</dep>
        </entry>
        <entry> 
          <ind>-30</ind>
          <dep>-12</dep>
        </entry>
        <entry> 
          <ind>-20</ind>
          <dep>2</dep>
        </entry>
        <entry> 
          <ind>-10</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>20</dep>
        </entry>
        <entry> 
          <ind>10</ind>
          <dep>28</dep>
        </entry>
        <entry> 
          <ind>20</ind>
          <dep>37</dep>
        </entry>
        <entry> 
          <ind>30</ind>
          <dep>43</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0.00229331</x-m>
        <y-m>0</y-m>
        <z-m>-0.01344</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.65</red>
        <green>0.10</green>
        <blue>0.10</blue>
      </emission>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
