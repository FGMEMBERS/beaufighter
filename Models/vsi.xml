<?xml version="1.0"?>
  <!-- vertical speed indicator -->
  <PropertyList> 
    <path>vsi.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>vsi-Needle1</object-name>
      <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
      <interpolation> 
        <entry>
          <ind>-5000</ind>
          <dep>-175</dep>
        </entry>
        <entry>
          <ind>-4000</ind>
          <dep>-170</dep>
        </entry>
        <entry>
          <ind>-3000</ind>
          <dep>-120</dep>
        </entry>
        <entry>
          <ind>-2000</ind>
          <dep>-75</dep>
        </entry>
        <entry>
          <ind>-1000</ind>
          <dep>-35</dep>
        </entry>
        <entry>
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry>
          <ind>1000</ind>
          <dep>35</dep>
        </entry>
        <entry>
          <ind>2000</ind>
          <dep>75</dep>
        </entry>
        <entry>
          <ind>3000</ind>
          <dep>120</dep>
        </entry>
        <entry>
          <ind>4000</ind>
          <dep>165</dep>
        </entry>
        <entry>
          <ind>5000</ind>
          <dep>170</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
      <shininess>10</shininess>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
