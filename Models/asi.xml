<?xml version="1.0"?>
  <!-- air speed indicator -->
  <PropertyList> 
    <path>asi.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>asi-Needle1</object-name>
      <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>34.75</ind>
          <dep>12</dep>
        </entry>
        <entry> 
          <ind>60.8</ind>
          <dep>30</dep>
        </entry>
        <entry> 
          <ind>86.9</ind>
          <dep>60</dep>
        </entry>
        <entry> 
          <ind>139.0</ind>
          <dep>155</dep>
        </entry>
        <entry> 
          <ind>173.8</ind>
          <dep>225</dep>
        </entry>
        <entry> 
          <ind>243.31</ind>
          <dep>342</dep>
        </entry>
        <entry> 
          <ind>260.69</ind>
          <dep>365</dep>
        </entry>
        <entry> 
          <ind>347.59</ind>
          <dep>480</dep>
        </entry>
        <entry> 
          <ind>417.11</ind>
          <dep>575</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <object-name>all</object-name>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
