      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <object-name>all</object-name>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
