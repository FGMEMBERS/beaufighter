<?xml version="1.0"?>
  <PropertyList> 
    <path>uc-indicator.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>blind</object-name>
      <property>gear/gear[1]/position-norm</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>-12</dep>
        </entry>
		<entry> 
          <ind>1</ind>
          <dep>12</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>-0.04</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
