<?xml version="1.0"?>
  <!-- turn and slip indicator -->
  <PropertyList> 
    <path>triple.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Supply-Needle1</object-name>
      <property>engines/engine/rpm</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>-40</dep>
        </entry>
        <entry> 
          <ind>250</ind>
          <dep>-30</dep>
        </entry>
        <entry> 
          <ind>500</ind>
          <dep>-20</dep>
        </entry>
        <entry> 
          <ind>750</ind>
          <dep>-10</dep>
        </entry>
        <entry> 
          <ind>1000</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1500</ind>
          <dep>5</dep>
        </entry>
        <entry> 
          <ind>2000</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>3000</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>4000</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>5000</ind>
          <dep>10</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>rotate</type>
      <object-name>L-Brake-Needle2</object-name>
      <property>controls/gear/brake-left</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>-75</dep>
        </entry>
        <entry> 
          <ind>0.5</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1</ind>
          <dep>75</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0.00234796</x-m>
        <y-m>-0.00395277</y-m>
        <z-m>-0.00856694</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>rotate</type>
      <object-name>R-Brake-Needle3</object-name>
      <property>controls/gear/brake-right</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>-75</dep>
        </entry>
        <entry> 
          <ind>0.5</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1</ind>
          <dep>75</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0.00234796</x-m>
        <y-m>0.00342677</y-m>
        <z-m>-0.00856694</z-m>
      </center>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
   <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
