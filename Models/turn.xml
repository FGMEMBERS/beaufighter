<?xml version="1.0"?>
  <!-- turn and slip indicator -->
  <PropertyList> 
    <path>turn.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Needle1</object-name>
      <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
      <interpolation> 
        <entry> 
          <ind>-3</ind>
          <dep>-30</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>3</ind>
          <dep>30</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>rotate</type>
      <object-name>Needle2</object-name>
      <property>instrumentation/turn-indicator/indicated-turn-rate</property>
      <interpolation> 
        <entry> 
          <ind>-4</ind>
          <dep>-60</dep>
        </entry>
        <entry> 
          <ind>-3</ind>
          <dep>-45</dep>
        </entry>
        <entry> 
          <ind>-2</ind>
          <dep>-30</dep>
        </entry>
        <entry> 
          <ind>-1</ind>
          <dep>-15</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1</ind>
          <dep>15</dep>
        </entry>
        <entry> 
          <ind>2</ind>
          <dep>30</dep>
        </entry>
        <entry> 
          <ind>3</ind>
          <dep>45</dep>
        </entry>
		<entry> 
          <ind>4</ind>
          <dep>60</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
      <shininess>10</shininess>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
