<?xml version="1.0"?>
  <PropertyList> 
    <path>fuel-pressure.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Fuel-Pressure-Needle1</object-name>
      <property>engines/engine/rpm</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>-25</dep>
        </entry>
        <entry> 
          <ind>250</ind>
          <dep>-20</dep>
        </entry>
        <entry> 
          <ind>500</ind>
          <dep>-15</dep>
        </entry>
        <entry> 
          <ind>750</ind>
          <dep>-10</dep>
        </entry>
        <entry> 
          <ind>1000</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1500</ind>
          <dep>5</dep>
        </entry>
        <entry> 
          <ind>2000</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>3000</ind>
          <dep>15</dep>
        </entry>
		<entry> 
          <ind>4000</ind>
          <dep>15</dep>
        </entry>
		<entry> 
          <ind>5000</ind>
          <dep>15</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>-0.04</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
      <shininess>10</shininess>
      
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
