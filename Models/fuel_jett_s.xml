<?xml version="1.0"?>
  <PropertyList> 
    <path>fuel_jett.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Handle</object-name>
      <property>/controls/fuel/jett-s</property>
      <factor>-90</factor>
      <center> 
        <x-m>0.008</x-m>
        <y-m>0</y-m>
        <z-m>0.008</z-m>
      </center> 
      <axis> 
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis> 
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
  </PropertyList>
