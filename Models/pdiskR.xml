<?xml version="1.0"?>

<PropertyList>
	
	<!--back-->
	<!--right -->
	<!--up -->
	
  <model>
    <name>pdisk</name>
    <path>Aircraft/mosquito/Models/pdisk.ac</path>
    <offsets>
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
      <pitch-deg>0.0</pitch-deg>
    </offsets>
  </model>	


 <animation>
  <type>select</type>
  <object-name>noshadow.spdisk</object-name>
  <condition>
   <less-than>
    <property>engines/engine[1]/rpm</property>
    <value>2001</value>
   </less-than>
   <greater-than>
    <property>engines/engine[1]/rpm</property>
    <value>1000</value>
   </greater-than>
  </condition>
 </animation>

<animation>
  <type>select</type>
  <object-name>noshadow.fpdisk</object-name>
  <condition>
   <greater-than>
    <property>engines/engine[1]/rpm</property>
    <value>2000</value>
   </greater-than>
  </condition>
 </animation>

<animation>
  <type>spin</type>
  <object-name>noshadow.spdisk</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor>0.35</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<animation>
  <type>spin</type>
  <object-name>noshadow.fpdisk</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor>0.01</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>


</PropertyList>
