<?xml version="1.0"?>

<!-- Bristol Beaufighter by Detlef Faber, Instruments by AJ MacLeod -->

<PropertyList>

 <path>beaufighter.ac</path>

  <nasal>
    <load>
      var update = aircraft.overlay_update.new();
      update.add("Aircraft/beaufighter/Models/AI-Liveries", "sim/model/livery/file");
      update.add("Aircraft/Generic/Logos", "sim/multiplay/generic/string");
    </load>
    <unload>
      update.stop();
    </unload>
  </nasal>

	<model>
		<name>cockpit</name>
		<path>Aircraft/beaufighter/Models/cockpit.xml</path>
	</model>
	<animation>
  <object-name>cockpit</object-name>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>10</max-m>
	</animation>
	<animation>
		<type>shader</type>
		<shader>chrome</shader>
		<texture>Aircraft/beaufighter/Models/glass_shader.png</texture>
		<object-name>cockpitglass</object-name>
		<object-name>cockpitdoorglass</object-name>
		<object-name>turretglass</object-name>
	</animation>

	<model>
		<nopreview/>
		<path>Aircraft/beaufighter/Models/equipment.xml</path>
	</model>


